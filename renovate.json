{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "groupName": "dagger",
      "matchPackagePrefixes": [
        "com.google.dagger:"
      ]
    },
    {
      "groupName": "kctfork",
      "matchPackagePrefixes": [
        "dev.zacsweers.kctfork:"
      ]
    },
    {
      "groupName": "kotlin",
      "matchPackageNames": [
        "com.google.devtools.ksp"
      ],
      "matchPackagePrefixes": [
        "org.jetbrains.kotlin:"
      ]
    }
  ]
}
